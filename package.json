  "private": true,
  "scripts": {
    "preinstall": "node -e \"if (process.env.npm_execpath && !process.env.npm_execpath.includes('bun')) {console.error('\\n⚠️  Error: This project uses Bun!\\n\\nPlease use: bun install\\n');process.exit(1); }\"",
    "dev": "next dev",
